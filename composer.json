{
    "name": "fredmansky/eventsky",
    "description": "Craft plugin for event management and attendee registration",
    "type": "craft-plugin",
    "version": "1.0.0-alpha.2",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "events",
        "eventsky",
        "event management"
    ],
    "support": {
    },
    "authors": [
        {
            "name": "Fredmansky",
            "homepage": "https://fredmansky.at/"
        }
    ],
    "require": {
        "craftcms/cms": "^3.2.0",
        "tightenco/collect": "^6.0",
        "nesbot/carbon": "^2.24"
    },
    "autoload": {
        "psr-4": {
          "fredmansky\\eventsky\\": "src/"
        }
    },
    "extra": {
        "name": "Eventsky",
        "handle": "eventsky",
        "hasCpSettings": true,
        "hasCpSection": true,
        "class": "fredmansky\\eventsky\\Eventsky"
    }
}
